{
  "name": "xion-deployed-contracts",
  "version": "1.0.0",
  "description": "Registry of deployed contracts on Xion blockchain",
  "main": "index.js",
  "scripts": {
    "validate": "node scripts/validate.js",
    "verify": "node scripts/verify-contracts.js",
    "validate:mainnet": "node scripts/unified-validator.js --validate-only --network=mainnet",
    "verify:mainnet": "node scripts/unified-validator.js --verify-only --network=mainnet",
    "validate:testnet": "node scripts/unified-validator.js --validate-only --network=testnet",
    "verify:testnet": "node scripts/unified-validator.js --verify-only --network=testnet",
    "validate-all:mainnet": "node scripts/unified-validator.js --network=mainnet",
    "validate-all:testnet": "node scripts/unified-validator.js --network=testnet",
    "test-validation": "node scripts/test-unified-validator.js",
    "migrate-testnet": "tsx scripts/migrate-to-testnet.ts",
    "build-site": "node scripts/build-site.js",
    "serve": "npx http-server docs -p 8080"
  },
  "keywords": [
    "xion",
    "cosmwasm",
    "smart-contracts",
    "blockchain"
  ],
  "author": "Burnt Labs",
  "license": "MIT",
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.36.1",
    "@cosmjs/proto-signing": "^0.36.1",
    "@cosmjs/stargate": "^0.36.1",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "http-server": "^14.1.1",
    "tsx": "^4.7.1"
  },
  "packageManager": "pnpm@9.15.4"
}